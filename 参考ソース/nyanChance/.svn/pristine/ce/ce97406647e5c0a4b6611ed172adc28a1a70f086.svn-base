    BIRTHDAY DATE NOT NULL,
    SEX CHAR(2),
    CAT_KIND VARCHAR(10) NOT NULL,
    IS_FOSTER_PARENTS BOOLEAN DEFAULT FALSE,
    COMMENT VARCHAR(100)
);

