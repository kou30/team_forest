package model;

import java.util.List;

import dao.CatDAO;

public class CatLogic {
	public List<Cat> findAll() {
		CatDAO dao = new CatDAO();
		List<Cat> catList = dao.findAll();
		return catList;
	}
	public List<Cat> findFin() {
		CatDAO dao = new CatDAO();
		List<Cat> catList = dao.findFin();
		return catList;
	}
	public void create(Cat cat) {
		CatDAO dao = new CatDAO();
		dao.create(cat);
	}
}
